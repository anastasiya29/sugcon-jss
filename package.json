{
  "name": "sugcon",
  "description": "DOT site",
  "version": "11.0.2",
  "private": true,
  "config": {
    "appName": "sugcon",
    "rootPlaceholders": [
      "sugcon-main"
    ],
    "sitecoreDistPath": "/dist/sugcon",
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "language": "en"
  },
  "engines": {
    "node": ">=8.1",
    "npm": ">=5.6.0",
    "yarn": "yarn is not supported, please use npm"
  },
  "author": {
    "name": "Altola, Inc."
  },
  "license": "SEE README",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@loadable/component": "^5.12.0",
    "@n8tb1t/use-scroll-position": "^1.0.43",
    "@sitecore-jss/sitecore-jss-react": "^13.0.5",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "cloudinary-react": "^1.5.0",
    "google-map-react": "^1.1.7",
    "node-fetch": "^2.6.0",
    "react": "^16.13.1",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-helmet": "~5.2.0",
    "react-lazyload": "^2.6.7",
    "react-loadable-visibility": "^3.0.2",
    "react-modal": "^3.11.1",
    "react-router-dom": "^5.1.2",
    "react-ssr-error-boundary": "^1.0.1",
    "react-youtube": "^7.11.2",
    "serialize-javascript": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/register": "^7.9.0",
    "@craco/craco": "^5.6.3",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@sitecore-jss/sitecore-jss-cli": "^13.0.5",
    "@sitecore-jss/sitecore-jss-dev-tools": "^13.0.3",
    "@sitecore-jss/sitecore-jss-manifest": "^13.0.3",
    "@svgr/webpack": "^4.3.3",
    "@types/loadable__component": "^5.10.0",
    "@types/loadable__server": "^5.9.1",
    "@types/node": "^10.14.7",
    "@types/node-fetch": "^2.5.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-helmet": "^5.0.7",
    "@types/react-loadable-visibility": "^3.0.0",
    "@types/react-modal": "^3.10.1",
    "@types/react-router-dom": "^5.1.5",
    "babel-preset-react-app": "^9.1.2",
    "bundlesize": "^0.18.0",
    "chalk": "~2.4.2",
    "chokidar": "~2.0.4",
    "cross-env": "~5.2.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "html-loader": "~0.5.5",
    "http-proxy-middleware": "~0.19.1",
    "npm-run-all": "~4.1.5",
    "null-loader": "~0.1.1",
    "patch-package": "^6.2.0",
    "preload-webpack-plugin": "3.0.0-beta.4",
    "react-scripts": "^3.4.1",
    "tailwindcss": "^1.3.5",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.7.4",
    "url-loader": "2.1.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "~3.3.10"
  },
  "scripts": {
    "jss": "jss",
    "dev": "npm-run-all --serial bootstrap:disconnected --parallel start:react start:proxy start:watch-components",
    "start:connected": "scjss-verify-setup && npm-run-all --serial bootstrap:connected start:react",
    "build": "npm-run-all --serial bootstrap:connected build:client build:server",
    "scaffold": "node scripts/scaffold-component.js",
    "start:react": "craco start",
    "start:proxy": "node scripts/disconnected-mode-proxy.js",
    "start:watch-components": "node scripts/generate-component-factory.js --watch",
    "build:client": "cross-env-shell NODE_ENV=development PUBLIC_URL=$npm_package_config_sitecoreDistPath \"craco build\"",
    "build:server": "cross-env-shell NODE_ENV=development PUBLIC_URL=$npm_package_config_sitecoreDistPath \"webpack --config server/server.webpack.config.js\"",
    "jss:manifest": "jss manifest -c -d --manifestSourceFiles ./src/**/*.sitecore.ts ./sitecore/definitions/**/*.sitecore.ts",
    "jss:deployitems": "cross-env-shell jss deploy items -c -d --manifestSourceFiles ./src/**/*.sitecore.ts ./sitecore/definitions/**/*.sitecore.ts --acceptCertificate $npm_package_config_cert",
    "jss:deploy": "cross-env-shell jss deploy app -c -d --manifestSourceFiles ./src/**/*.sitecore.ts ./sitecore/definitions/**/*.sitecore.ts --acceptCertificate $npm_package_config_cert",
    "bootstrap:connected": "node scripts/bootstrap.js",
    "bootstrap:disconnected": "node scripts/bootstrap.js --disconnected",
    "eject": "react-scripts eject",
    "perfbudget": "bundlesize --config bundlesize.config.json",
    "analyze": "cross-env BUNDLE_ANALYZE=true npm run build",
    "server:render": "node server/server-bundle-renderer.js",
    "postinstall": "patch-package"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
